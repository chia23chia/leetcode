class Solution {
public:
    int minPartitions(string n) {
        int ans = 0;
        for(int i = 0; i < n.size() ; i++)
        {
            if(n[i] == '1')
                if(ans <= 1)
                    ans = 1 ;
            if(n[i] == '2')
                if(ans <= 2)
                    ans = 2 ;
            if(n[i] == '3')
                if(ans <= 3)
                    ans = 3 ;
            if(n[i] == '4')
                if(ans <= 4)
                    ans = 4 ;
            if(n[i] == '5')
                if(ans <= 5)
                    ans = 5 ;
            if(n[i] == '6')
                if(ans <= 6)
                    ans = 6 ;
            if(n[i] == '7')
                if(ans <= 7)
                    ans = 7 ;
            if(n[i] == '8')
                if(ans <= 8)
                    ans = 8 ;
            if(n[i] == '9')
                if(ans <= 9)
                    ans = 9 ;
            if(n[i] == '0')
                if(ans <= 0)
                    ans = 0 ;
        }
        return ans ;
    }
};
