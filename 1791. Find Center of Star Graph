class Solution {
public:
    int findCenter(vector<vector<int>>& edges) {
        int ans ;
        int t1 = edges[0][0];
        int t2 = edges[0][1];
        for(int i = 0 ; i < 2 ; i++)
        {
            if(edges[1][i] == t1)
                ans = t1 ;
            if(edges[1][i] == t2)
                ans = t2 ;
        }
        return ans ;
    }
};
